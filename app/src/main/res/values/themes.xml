<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.kucekjemur" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/primary_blue</item>
        <item name="colorPrimaryVariant">@color/primary_blue</item>
        <item name="colorOnPrimary">@color/primary_blue</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/secondary_orange</item>
        <item name="colorSecondaryVariant">@color/secondary_orange</item>
        <item name="colorOnSecondary">@color/secondary_orange</item>
    </style>

    <style name="kucekjemur.NoActionBar" parent="Theme.kucekjemur">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="LayarTancepXXI.NoActionBar.FullScreen" parent="kucekjemur.NoActionBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="TransparentStatusBarTheme" parent="Theme.kucekjemur">
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:windowTranslucentStatus">true</item>
    </style>

    <style name="Theme.kucekjemur" parent="Base.Theme.kucekjemur" />

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
</resources>